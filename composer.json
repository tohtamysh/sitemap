{
  "name": "tohtamysh/sitemap",
  "description": "Create sitemap",
  "minimum-stability": "dev",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Tohtamysh",
      "email": "tohtamysh@gmail.com"
    }
  ],
  "homepage": "https://github.com/tohtamysh/sitemap",
  "require": {
    "php": "^7.2.5|^8.0",
    "ext-simplexml": "*",
    "nesbot/carbon": "^2.0"
  },
  "require-dev": {
